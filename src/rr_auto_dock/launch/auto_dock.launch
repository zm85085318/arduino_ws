<launch>
  <node pkg="aruco_detect" name="aruco_detect"
    type="aruco_detect" output="screen" respawn="false">
    <param name="publish_images" value="false" />
    <param name="fiducial_len" value="0.13"/>
    <param name="dictionary" value="7"/>
    <param name="do_pose_estimation" value="true"/>
  </node>

  <node pkg="rr_auto_dock" type="auto_dock.py" name="auto_dock_node">
      <param name="dock_aruco_number" value="102"/>
      <param name="cmd_vel_angular_rate" value="0.2" />
      <param name="cmd_vel_linear_rate" value="0.3" />
      <!-- <param name="cmd_search_angular_rate" value="0.2" /> -->
      <param name="motor_response_delay" value="0.1" />
      <param name="search_turn_amount" value="0.3" />
      <param name="undock_distance" value="0.2" />

  </node>

</launch>